{
  "name": "prismarine-gym",
  "version": "0.1.0",
  "description": "A JavaScript Environment for Reinforcement Learning in Minecraft",
  "main": "dist/env.js",
  "types": "dist/env.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --coverage --coverageReporters=text-lcov",
    "format": "prettier --write \"src/**/*.ts\" \"lib/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "demo": "webpack --watch & live-server --port=8085",
    "docs": "typedoc ./lib"
  },
  "keywords": [
    "reinforcement learning"
  ],
  "author": "Louis Beaumont",
  "repository": {
    "type": "git",
    "url": "https://github.com/louis03195/prismarine-gym.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "coveralls": "^3.0.7",
    "jest": "^24.8.0",
    "live-server": "^1.2.1",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.4.5",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.14.2",
    "typedoc-plugin-markdown": "^2.0.6",
    "typescript": "^3.9.7",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4",
    "flying-squid": "1.3.1"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "gym-js": "file:../gym.js",
    "@tensorflow/tfjs": "2.6.0",
    "mineflayer": "2.28.1"
  }
}
